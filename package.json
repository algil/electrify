{
  "name": "electrify",
  "version": "0.0.0",
  "author": "Vladimir Krivosheev <develar@gmail.com>",
  "description": "Step-by-step wizard to prepare Electron app for distribution, from packaging to auto-update",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "build": "node .electron-vue/build.js && electron-builder --dir -c.compression=store",
    "build:web": "node .electron-vue/build.js --web",
    "build:clean": "node .electron-vue/build.js --clean",
    "dist": "yarn build && electron-builder",
    "lint": "tslint -c ./node_modules/electron-builder-tslint-config/tslint.json -p src/renderer",
    "lintAndFix": "tslint -c ./node_modules/electron-builder-tslint-config/tslint.json -p src/renderer --fix"
  },
  "build": {
    "extends": ".electron-vue/electron-builder.yml",
    "productName": "Electrify",
    "appId": "build.electron.electrify"
  },
  "dependencies": {
    "bluebird-lst": "^1.0.2",
    "clone": "^2.1.1",
    "debug": "^2.6.8",
    "deep-diff": "^0.3.8",
    "electron-debug": "^1.2.0",
    "electron-store": "^1.2.0",
    "fs-extra-p": "^4.3.0",
    "iview": "^2.0.0-rc.18",
    "node-watch": "^0.5.5",
    "shell-env": "^0.3.0",
    "vue": "^2.3.4",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.7.0",
    "xstream": "^10.8.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babili-webpack-plugin": "^0.1.2",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron": "1.7.4",
    "electron-builder": "^19.13.0",
    "electron-builder-tslint-config": "^1.0.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0-beta.3",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.4",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.4",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.2",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "vue-class-component": "^5.0.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.1"
  }
}
